// prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// Admin table
model Admin {
  id       Int     @id @default(autoincrement())
  name     String
  email    String  @unique
  password String
  role     String
}
